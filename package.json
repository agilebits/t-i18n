{
  "name": "t-i18n",
  "version": "0.6.1",
  "description": "Simple, standards-based localization",
  "author": "Mitch Cohen <mitch.cohen@me.com>",
  "homepage": "https://github.com/agilebits/t-i18n#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:agilebits/t-i18n.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "cd dist/; rm -rf *",
    "build": "npm run clean; cd src/; tsc -d; tsc -d -p tsconfig.es6.json; cd ../scripts; tsc",
    "prepublishOnly": "npm run build",
    "test": "mocha -r ts-node/register ./test/*.test.ts"
  },
  "bin": {
    "extract-strings": "./scripts/extract-strings.js"
  },
  "keywords": [
    "i18n",
    "l10n",
    "globalization",
    "translation",
    "icu"
  ],
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.0.24",
    "chai": "^4.1.1",
    "glob": "^7.1.2",
    "mocha": "^5.1.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.8.1",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  }
}
